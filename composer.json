{
    "name": "codebrewery/untappd",
    "description": "PHP SDK for Untappd",
    "keywords": [],
    "license": "MIT",
    "authors": [
        {
            "name": "Mattias Lyckne",
            "email": "mattias@lyckne.se"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodeBrewery\\Untappd\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeBrewery\\Untappd\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpspec run",
        "test-coverage": "phpspec run -c phpspec-with-coverage.yml"
    }
}
